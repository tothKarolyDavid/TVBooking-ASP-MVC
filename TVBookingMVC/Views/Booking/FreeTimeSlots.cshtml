@model System.Collections.Generic.List<TVBookingMVC.Controllers.FreeTimeSlot>

@{
    ViewBag.Title = "Free time slots";
    Layout = "_Layout";
}

<h2>Free time slots for the next 7 days</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                Available from
            </th>
            <th>
                Available to
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Start)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.End)
                </td>
            </tr>
        }
    </tbody>
</table>

